        {{#link-to "review" "5de5621737b72e001931a978" class="nav-item nav-link"}}TEST: Review{{/link-to}}
        {{#link-to "logout" class="nav-item nav-link"}}Log out{{/link-to}}
      {{/if}}
    </ul>
  </nav>
  {{loading-progress class="bg-dark" progressBackground="bg-success"}}
</div>

{{ember-notify}}

<div class="container my-3">
  <div class="row">
    <div class="col">
      {{outlet}}
    </div>
  </div>
</div>
