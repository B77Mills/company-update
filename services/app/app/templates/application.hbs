      <img
        src={{ config.logo }}
        class="navbar-brand__logo"
        alt="Publisher Logo"
      >
    </span>
    <div class="navbar-nav mr-auto">
      {{outlet "nav"}}
    </div>
    <ul class="navbar-nav ml-auto">
      <li class="nav-item">
        <a href="{{ config.domain }}/contact-us" target="_blank" rel="noopener" class="nav-link">Contact Us</a>
      </li>
      {{#if session.isAuthenticated}}
        {{#link-to "list" class="nav-item nav-link"}}List{{/link-to}}
