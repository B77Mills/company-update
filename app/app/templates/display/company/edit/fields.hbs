{{#each fields as |row|}}
  <div class="row">
    {{#each row as |field|}}
      {{ edit-field field=field value=(get model field.key)}}
    {{/each}}
  </div>
{{/each}}
{{#if config.isPmmi}}
  {{#each extra as |row|}}
    <div class="row">
      {{#each row as |field|}}
        {{ edit-field field=field value=(get model field.key)}}
      {{/each}}
    </div>
  {{/each}}
{{/if}}
